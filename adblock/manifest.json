{
    "author": "muyuren",
    "description": "Get information of the active tab.",
    "icons":
      {
        "48": "icons/microsoft.png",
        "96": "icons/microsoft-96.png"
      },
    "manifest_version": 2,
    "name": "edgeExt",
    "version": "1.0",
    "permissions": [
      "tabs"
    ],
    "browser_action": {
      "default_icon": {
        "30": "icons/microsoft-30.png"
      },
      "default_title": "edgeExt",
      "default_popup": "popup.html"
    }
  }